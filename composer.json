{
    "name": "zitec/zitec-bundles",
    "description": "Bundles with extra features",
    "type": "library",
    "require": {
        "php": "^8.1",
        "doctrine/doctrine-bundle": "^2.9",
        "doctrine/orm": "^2.15",
        "symfony/twig-bundle": "^6.2",
        "symfony/framework-bundle": "^6.2",
        "symfony/property-access": "^6.2",
        "symfony/form": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "authors": [
        {
            "name": "Ionut Breta",
            "email": "ionut.breta@zitec.com"
        }
    ],
    "config": {
        "discard-changes": true,
        "platform": {
            "php": "8.1"
        }
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    }
}
